<?xml version="1.0"?>
<launch>
	<arg name="robot_ip" default="192.168.0.111" doc="IP address by which the robot can be reached."/>
  <arg name="kinematics_config" default="$(find ur_teleop_controller)/calibration/ur5e_calibration.yaml" doc="Kinematics config file used for calibration correction. This will be used to verify the robot's calibration is matching the robot_description."/>

  <include file="$(find ur_robot_driver)/launch/ur5e_bringup.launch" pass_all_args="true" />
</launch>
